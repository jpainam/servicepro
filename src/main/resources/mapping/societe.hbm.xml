<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.cfao.app.beans.Societe" table="societe">
        <id name="idsociete" type="int" column="idsociete">
            <generator class="assigned"/>
        </id>
        <property name="nom">
            <column name="nom"/>
        </property>
        <property name="code">
            <column name="code"/>
        </property>
        <property name="contact">
            <column name="contact"/>
        </property>
        <property name="telephone">
            <column name="telephone"/>
        </property>
        <property name="email">
            <column name="email"/>
        </property>
        <property name="fax">
            <column name="fax"/>
        </property>
        <property name="adresse">
            <column name="adresse"/>
        </property>
    </class>
</hibernate-mapping>